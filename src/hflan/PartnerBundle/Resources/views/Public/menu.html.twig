<hr>
<h3>
    {{ "sidebar.partners.title"|trans({}, "sidebar")|desc("Titre menu : Partenaires") }}
</h3>
<div class="partners">
    {% for partner in partners %}
        <a href="{{ partner.url }}" title="{{ partner.text }}" rel="nofollow">
            <img src="{{ asset(partner.webPath) }}" alt="[{{ partner.name }}]">
        </a>
    {% endfor %}
</div>